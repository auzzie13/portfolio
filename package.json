    "concurrently": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.8.10",
